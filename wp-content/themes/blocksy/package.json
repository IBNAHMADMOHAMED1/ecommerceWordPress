{
	"name": "small-ct",
	"version": "1.8.31",
	"title": "Blocksy",
	"browserslist": "> 1%, not opera > 0, not op_mini all",
	"devDependencies": {
		"@babel/plugin-transform-parameters": "^7.13.0",
		"@octokit/core": "^3.2.5",
		"babel-plugin-lodash": "^3.3.4",
